import java.io.*;
import java.net.*;
import java.util.StringTokenizer;


public class MakeName {
	Socket socket;
	StringTokenizer st;
	
	String nameTag = "NAME";
	
	
	MakeName(String _m, Socket _s){
		String msg = _m;
		socket = _s;
		
		st = new StringTokenizer(msg, "!");
		
		String tag = st.nextToken();
		String name = st.nextToken();
		
		
	}
}
